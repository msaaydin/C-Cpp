#ifndef PERSONEL_H_
#define PERSONEL_H_

#include <string>
using namespace std;

class Personel {
public:
    void setName(string);
    string getName();
private:
    string p_name;
};
string Personel::getName(){
   return this->p_name;
}
#endif /* PERSONEL_H_ */
