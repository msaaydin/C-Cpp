//soru 6:
int main(void){

    int array1[] = {134,52,653,344,454,675,99,232};
    int k;
    for(k = 0; k < 8; k++)
	printf("%d - ",array1[k]);
	printf("\n");
	printf ("**************************************\n");
    int array2[] = {23,34,65,76,87,23,8,2};
	int *ip1,*ip2,*ep;
	ip2 = &array2[8];
	ep = &array1[8];
	  for(ip1 = &array1[0]; ip1 < ep; ip1++){
                           *ip2 = *ip1;
                              ip2--;
	  }
	  for(k = 0; k < 8; k++)
	  	 printf("%d - ",array2[k]);

	return 0;

}



