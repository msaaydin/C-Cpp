#ifndef PERSONEL_H_
#define PERSONEL_H_
#include <string>
#include <iostream>

using namespace std;
class Personel {
public:
	string ad, soyad;
    double maas;
	Personel();
	Personel(string ad, string soyad, double maas){
			this->ad = ad;
			this->soyad = soyad;
			this->maas = maas;
	}

};

#endif /* PERSONEL_H_ */
