#include <stdio.h>
#include <stdlib.h>

/*int main(void) {
	puts("Midterm 2 solution"); /* prints Midterm 2 solution */
	return EXIT_SUCCESS;

}*/
int main(void){

	char str[80] = {"Fatih sul tan mehmet vakif uni"};
	    char token[10];
	    char *p , *q;
	    p = str;
	    while (*p) {
	        q = token;
	        while (*p != ' ' && *p) {
	            *q = *p;
	             q++ ; p++;
	        }
	        if (*p) p++;
	        *q = '\0';
	        printf("%s\n" , token);
	    }
	    return 0;
	}

	return 0;

}
