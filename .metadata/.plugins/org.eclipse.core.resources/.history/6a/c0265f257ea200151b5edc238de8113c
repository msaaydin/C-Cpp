//soru 5:
/*
 * output:
 * 45 (000000000023FE00)
   67 (000000000023FE04)
   89 (000000000023FE08)
 * */
int main(void){
	int array[] = { 45, 67, 89 };
	    int * array_ptr;
	    array_ptr = array;
	    printf("%i (%p)\n", *array_ptr, array_ptr);
	    array_ptr++;
	    printf("%i (%p)\n", *array_ptr, array_ptr);
	    array_ptr = array_ptr + 1;
	    printf("%i (%p)\n", *array_ptr, array_ptr);
	    return 0;


}


