#ifndef TEMPLATESTACK_H_
#define TEMPLATESTACK_H_

template<class T>
class TemplateStack {
public:
	  TemplateStack(int);
      void push(T i);
      T pop();
  private:
      int top;
      int size;
      T st[size];
};
template <class T>
TemplateStack<T>::TemplateStack()
{
       top = -1;
}
#endif /* TEMPLATESTACK_H_ */
