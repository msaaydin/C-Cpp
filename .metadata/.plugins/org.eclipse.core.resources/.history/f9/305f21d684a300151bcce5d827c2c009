#ifndef TEMPLATESTACK_H_
#define TEMPLATESTACK_H_

template<typename T>
class TemplateStack {
public:
	  TemplateStack(int);
      void push(T i);
      T pop();
  private:
      int top;
      int size;
      T st[size];
};

#endif /* TEMPLATESTACK_H_ */
