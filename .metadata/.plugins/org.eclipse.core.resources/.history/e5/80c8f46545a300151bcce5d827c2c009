#include <iostream>
#include "Personel.h"
#include <string>

using namespace std;
void swap(int &s1,int &s2){

	int t = s1;
	s1 = s2;
	s2 = t;
}
int main() {
	cout << "Cpp class example" << endl; // prints Cpp class example
	string a = "musa aydýn";
	cout<<a<<endl;
	Personel p("merhaba");
	p.setName("deneme");
    cout<<p.getName()<<endl;
    int ss1 = 34, ss2 = 45;
    cout<<"ss1 in deðeri...:"<<ss1<<"  ss2 nin deðeri....:"<<ss2<<endl;
    swap(ss1,ss2);
    cout<<"ss1 in deðeri...:"<<ss1<<"  ss2 nin deðeri....:"<<ss2<<endl;


	return 0;
}
