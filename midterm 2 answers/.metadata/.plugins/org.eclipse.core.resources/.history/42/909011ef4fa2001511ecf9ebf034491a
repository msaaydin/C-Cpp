struct Personel{
   char *ad_soyad;
   int yas;
   char cinsiyet;
};
#include <stdio.h>
#include <stdlib.h>

char *ptr(char *d1, char *d2){

	char *dp = &d1[0], *sp = &d2[0];
		while(*sp != '\0')
			*dp++ = *sp++;
		*dp = '\0';
		return dp;
}

int main(void) {
	struct Personel p[5];

	p[0].yas = 12;
	printf("%d\n",p[0].yas);
	puts("!!!Hello World!!!"); /* prints !!!Hello World!!! */
	char ch1[] ="marmara üniversitesi";
	char ch2[100];
	char *ch3 = ptr(ch2,ch1);
	int k;
	for(k = 0; k<sizeof(ch3); k++)
		printf("%c",ch3[k]);
	//puts(ch3);
	return EXIT_SUCCESS;
}


